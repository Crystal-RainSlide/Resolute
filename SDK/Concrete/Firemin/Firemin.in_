[Firemin]
PortableEdition=1
CheckForUpdates=4
Language=en
FirstRun=1
ShowNotifications=1
BoostEnabled=1
Boost=1000
LimitEnabled=1
ReduceLimit=20
StartBrowser=0
EnableExtendedProcs=1
ExtendedProcs=thunderbird.exe,sunbird.exe,seamonkey.exe,plugin-container.exe
BrowserPath=%ProgramFiles%\Mozilla Firefox\firefox.exe
ProcessPriority=4
SaveRealtime=0
ReduceMemory=1
ReduceEveryMill=300
MinSysMemoryPerc=80
[Safemode]
firefox.exe=-safe-mode
[Donate]
Seconds=0
